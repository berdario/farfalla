{
  "name": "Farfalla project",
  "version": "0.1",
  "manifest_version": 1,
  "description": "Farfalla Accessibility Toolbar",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["farfalla.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}